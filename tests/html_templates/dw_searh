if (window.location.hash != "") {
var redirectLink = window.location.hash.substring(0, window.location.hash.indexOf('?')).replace('#', "");
var hostPart = "http://" + window.location.host;
if (redirectLink.search('search') < 0) {
hostPart = window.location.pathname;
}
window.location.href = hostPart + redirectLink;
}
</script>
